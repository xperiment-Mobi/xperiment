		<template block="40" trials="32">
		

			<addJPG timeStart="1000" timeEnd="2000" depth="top" peg="refFood" copyOverID="ref" exactSize="true"  extension="png" filename="800;300"  if="this.doNow?results.refFood=this.filename"  />
			<addJPG timeStart="0" timeEnd="2000" extension="png" peg="refPlate" filename="plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate"  if="this.doBefore?results.refPlate=this.filename"/>
			
			
			<addShape peg="box" timeStart="0" width="1024" height="70%" colour="none" visible="false" y="35%"/>
			
			<behavRandPos usePegs="refFood,refPlate,box" link="refPlate,refFood" box="box"/>

			<behavSequence timeStart="2000" firstStim="true" mousePos="true" sortBy="sortMe" usePegs="images" hideAtStart="true" show="max;min" peg="mySeq"  />
			
			<behavResults copyOverID="results" peg="results"/>
			
			
			<addJPG timeStart="3000" howMany="50"  copyOverID="images" exactSize="true" extension="png" sortMe="0---1---etc---" filename="20---40---etc---" peg="images"/>
			
			<addJPG timeStart="2000" extension="png" peg="plate" filename="largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;largePlate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate;plate" present="true;true" if="this.doNow?results.plate=this.filename"/>
			
			<addText timeStart="2000" text="Press space when the amount of food above is the same as seen just previously."  peg="info2" width="80%" height="5%" align="center" vertical="bottom" fontSize="20" y="100%" />
			
			<addText timeStart="2000" peg="info"  text="Please change how much food by moving the mouse around either upward or downward." align="center" 
		fontSize="16" width="70%" y="10%" />
		
			<addText peg="info_dot"  text="Please move your mouse cursor into the circle to begin the next trial." align="center" 
		fontSize="25" width="100%" y="10%" />
		
			<addShape shape='circle' colour="grey" width="50" if="this.mouseOver?next.start()" shuffleSomething='y ;' y="80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%;80%;50%" alpha=".5" height="50" peg="mouseOver"/>
			
			<behavNextTrial peg="next"/>
			
			<addButton timeStart="2000" hideResults="true" visible="false" goto="" if="this.click?mySeq.stop(),info.stop(),info2.stop(),plate.stop(),info_dot.start(),mouseOver.start()" key=" " y="15%" width="200"  height="40"/>
		 
		
		</template>		
		



<TRIAL TYPE="Trial" hideResults="true" block="100" trials="1">
	<behavSaveData timeStart='0'/>
	<addText  wordWrap="true" text="Thanks for taking part in this study. We're exploring how the size of the plate some is placed on influences perceived portion size. If you have any questions or would like to here more about our research, please contact us at Bobby_Cheon@sics.a-star.edu.sg.
Press escape to leave full screen. You can now close this browser window. Thanks again!"	timeStart="0" width="80%"  align="left" timeEnd="forever" fontSize="20"></addText>

</TRIAL>
  	

</expt>	

