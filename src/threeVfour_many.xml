	<addJPG howMany="2" peg="round1---round2" extension="png" filename="roundPlate" timeStart="100" exactSize="false" width="320" height="320"/>
	

	<behavShapePattern copyOverID="p3" peg="p3" seperation="55" shape="{random:10}" background="round1" x="30%" timeStart="100" width="120" height="120" fillParentRatio=".22"/>
	<behavShapePattern copyOverID="p4" peg="p4" seperation="55" shape="{random:10}" background="round2" x="70%" timeStart="100" width="120" height="120" fillParentRatio=".22"/>
	
	<behavSelect copyOverID="dv" usePegs="p3,p4" timeStart="0" peg="nicest" if="this.onFinish?next.start()"/>
	<behavNextTrial peg="next" />
	
</template>

